module zvn (
    input x[16],
    input y[16],
    input alufn[6],
    input out[16],
    
    output z,
    output v,
    output n
  ) {

  
  always {
    
  z = ~|out;
  v = (x[15] & (y[15] ^ alufn[0]) & ~out[15]) | (~x[15] & ~(y[15] ^ alufn[0]) & out[15]);
  n = out[15];
    
  }
}