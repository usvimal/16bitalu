module comparator (
    input x[16],
    input y[16],
    input alufn[6],
    
    output out[16]
  ) {

  always {
    case (alufn[2:1]) {

          b01: // CMPEQ
            out[0] = (x == y);

          b10: // CMPLT
            out[0] = (x < y);

          b11: //CMPLE
            out[0] = (x <= y);

          default:
            out[0] = 0;
        }
  }
}
